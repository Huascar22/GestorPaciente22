<div class="row mb-2">
    <div class="col-3">
        <a asp-action="Crear" asp-controller="Examen" class="btn btn-primary">Crear Examen</a>
    </div>
    <div class="col-4">
        <form class="form-check d-flex" asp-action="BuscarByCedula" asp-controller="Examen">
            <input class="form-control" placeholder="Cedula" name="Cedula" />
            <button class="btn btn-primary ms-2">Buscar</button>
        </form>
    </div>
</div>


<div class="mt-5 d-flex justify-content-center alert alert-danger text-center">
    <h4>No hay Examenes Pendientes disponibles para esta cedula</h4>
</div>